{% extends 'base.html.twig' %}

{% block title %}GallerySetting{% endblock %}

{% block body %}
    <h1>GallerySetting</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ gallery_setting.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ gallery_setting.name }}</td>
            </tr>
            <tr>
                <th>Width</th>
                <td>{{ gallery_setting.width }}</td>
            </tr>
            <tr>
                <th>Height</th>
                <td>{{ gallery_setting.height }}</td>
            </tr>
            <tr>
                <th>Path</th>
                <td>{{ gallery_setting.path }}</td>
            </tr>
            <tr>
                <th>IsDefault</th>
                <td>{{ gallery_setting.isDefault ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_gallery_setting_index') }}">back to list</a>

    <a href="{{ path('app_gallery_setting_edit', {'id': gallery_setting.id}) }}">edit</a>

    {{ include('gallery_setting/_delete_form.html.twig') }}
{% endblock %}
